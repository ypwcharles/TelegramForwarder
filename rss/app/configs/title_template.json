{
    "patterns": [
      {
        "pattern": "^(?:#\\S+\\s*)+\\n\\s*\\n([^\\n]+)",
        "description": "第一行全是标签后的标题：#标签1 #标签2\\n\\n标题内容"
      },
      {
        "pattern": "^#[^\\s]+\\s+\\*\\*([^\\*]+?)\\*\\*",
        "description": "带标签的粗体标题：#标签 **标题**"
      },
      {
        "pattern": "^#[^\\s]+\\s+(.+?)(?=\\n|$)",
        "description": "带标签的标题：#标签 标题内容"
      },
      {
        "pattern": "^\\[\\*\\*([^\\*]+?)\\*\\*\\]\\([^\\)]+?\\)",
        "description": "带链接的粗体标题：[**标题**](链接)"
      },
      {
        "pattern": "^\\[([^\\]]+?)\\]\\([^\\)]+?\\)",
        "description": "带链接的标题：[标题](链接)"
      },
      {
        "pattern": "^\\*\\*([^\\*]+?)\\*\\*",
        "description": "粗体标题：**标题**"
      },
      {
        "pattern": "^【([^】]+?)】",
        "description": "中文方括号标题：【标题】"
      },
      {
        "pattern": "^\\[([^\\]]+?)\\]",
        "description": "中括号标题：[标题]"
      },
      {
        "pattern": "^(.+?)\\n",
        "description": "第一行作为标题"
      }
    ]
  } 